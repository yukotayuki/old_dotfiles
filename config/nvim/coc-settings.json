{
  "eslint.enable": true,
  "eslint.filetypes": ["javascript", "vue"],
  "eslint.run": "onType",
  "eslint.autoFixOnSave": true,
  "json.enable": true,
  "json.format.enable": false,
  "suggest.floatEnable": true,
  "vimlsp.diagnostic.enable": false,
  "tsserver.enable": false,
  "coc.preferences.formatOnSaveFiletypes": ["json", "rust"],
  "coc.source.emoji.filetypes": ["markdown", "vue", "javascript"],
  "tabnine.priority": 100,
  "explorer.icon.enableNerdfont": true,
  "explorer.presets": {
    "floating": {
      "position": "floating"
    },
    "home": {
      "root-uri": "~"
    }
  },
  "explorer.keyMappings": {
    "u": "gotoParent"
  },
  "languageserver": {
    "nim": {
      "command": "nimlsp",
      "filetypes": ["nim"]
    }
  }
}
